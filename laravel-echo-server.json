{
    "authHost": "https://server.local",
    "authEndpoint": "/graphql/subscriptions/auth",
    "clients": [],
    "database": "redis",
    "databaseConfig": {
        "redis": {
            "port": "6379",
            "host": "127.0.0.1",
            "options": {
                "password": null
            }
        },
        "sqlite": {
            "databasePath": "/database/laravel-echo-server.sqlite"
        },
        "publishPresence": true
    },
    "devMode": "true",
    "host": "0.0.0.0",
    "port": "3000",
    "protocol": "https",
    "sslCertPath": "C:/xampp/apache/conf/ssl.crt/server.crt",
    "sslKeyPath": "C:/xampp/apache/conf/ssl.key/server.key",
    "socketio": [],
    "secureOptions": 67108864,
    "sslCertChainPath": "",
    "sslPassphrase": "",
    "subscribers": {
        "http": true,
        "redis": true
    },
    "apiOriginAllow": {
        "allowCors": true,
        "allowOrigin": "*",
        "allowMethods": "POST",
        "allowHeaders": "Origin, Content-Type, X-Auth-Token, X-Requested-With, Accept, Authorization, X-CSRF-TOKEN, X-Socket-Id"
    }
}
